<h1>New Link</h1>

<form class="" action="<%= links_url  %>" method="post">
  <%= auth_token %>

  <label for="">Title
    <input type="text" name="link[title]" value="<%= @link.title %>">
  </label>

  <label for="">URL
    <input type="text" name="link[url]" value="<%= @link.url %>">
  </label>

  <input type="submit" name="" value="Create New Link">

</form>
